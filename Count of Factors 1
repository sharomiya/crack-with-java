import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int c = 0;
        for(int i=1;i<=n;i++)
        {
            if(n%i==0){
            c++;
            }
        }
        System.out.println(c);
    }
}
